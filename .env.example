# OpenAI API key for enhanced mapping and natural language query processing
# Get from https://platform.openai.com/account/api-keys
OPENAI_API_KEY=your_openai_api_key_here

# Flask secret key for session management
# Generate a random string, e.g.: python -c "import secrets; print(secrets.token_hex(16))"
SECRET_KEY=your_flask_secret_key_here

# Google API settings
# You don't need to set these if using OAuth flow or service account
# These are optional alternative ways to configure Google API access
GOOGLE_CLIENT_ID=your_google_client_id_here
GOOGLE_CLIENT_SECRET=your_google_client_secret_here
GOOGLE_CREDENTIALS_FILE=path_to_credentials_file 